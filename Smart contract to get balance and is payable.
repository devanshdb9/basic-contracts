pragma solidity ^0.4.0;
contract Mycontract
{
    uint myv;
    
    function Mycontract() payable{
    myv=5;
        
    }
    
    function setmyv(uint myvnew)
    {
        myv=myvnew;
        
        
    }
    function getmyv() constant returns(uint)
    {
        return myv;
    }
    function getmycontractbalance() constant returns(uint){
        return this.balance;
         }
    function() payable{
        
    }
}
